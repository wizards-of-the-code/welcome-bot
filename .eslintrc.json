{
    "extends": [
        "airbnb",
        "airbnb-typescript/base"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
        "linebreak-style": 0,
        "no-underscore-dangle" : 0,
        "no-useless-escape": "off"
    }
}
